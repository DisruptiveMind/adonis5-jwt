  "repository": {
    "type": "git",
    "url": "git"
  },
  "author": "Max Galbusera",
  "license": "MIT",
  "keywords": [
    "adonis",
    "adonis-auth",
    "authentication",
    "jwt"
  ],
  "devDependencies": {
    "@adonisjs/mrm-preset": "^4.1.2",
    "@adonisjs/require-ts": "^2.0.8",
    "@types/node": "^16.10.3",
    "del-cli": "^4.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.3",
    "eslint-plugin-prettier": "^4.0.0",
    "japa": "^3.1.1",
    "mrm": "^3.0.9",
    "np": "^7.5.0",
